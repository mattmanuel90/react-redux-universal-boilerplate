    "babel-preset-react": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
